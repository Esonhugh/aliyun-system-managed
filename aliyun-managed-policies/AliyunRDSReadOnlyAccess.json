{
  "Version": "1",
  "Statement": [
    {
      "Action": "rds:Describe*",
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Action": [
        "hdm:Get*",
        "hdm:Describe*",
        "hdm:Query*",
        "hdm:Support*"
      ],
      "Effect": "Allow",
      "Resource": "acs:rds:*:*:*"
    },
    {
      "Action": [
        "dbs:Get*",
        "dbs:Describe*",
        "dbs:Query*",
        "dbs:Support*"
      ],
      "Resource": "acs:rds:*:*:*",
      "Effect": "Allow"
    }
  ]
}