{
    "Version":"1",
    "Statement":[
        {
            "Action":"adcp:*",
            "Resource":"*",
            "Effect":"Allow"
        },
        {
            "Action":"ram:CreateServiceLinkedRole",
            "Resource":"*",
            "Effect":"Allow",
            "Condition":{
                "StringEquals":{
                    "ram:ServiceName":"adcp.aliyuncs.com"
                }
            }
        }
    ]
}